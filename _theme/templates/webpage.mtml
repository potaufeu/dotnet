<!DOCTYPE html><$mt:Include module="config"$>
<html lang="<$mt:BlogLanguage$>" itemscope itemtype="http://schema.org/WebPage">
<head>
<meta charset="<$mt:PublishCharset$>">
<title><$mt:PageTitle encode_html="1"$> <mt:If tag="pagefolder">| <mt:PageFolder><$mt:FolderLabel encode_html="1"$></mt:PageFolder> </mt:If>| <$mt:BlogName encode_html="1"$></title>
<meta name="viewport" content="width=device-width">
<meta name="description" content="<$mt:BlogDescription remove_html="1" encede_html="1"$>">
<meta name="keywords" content="<$mt:GetVar name="metakeywords"$>">
<link rel="shortcut icon" href="<$mt:BlogURL encode_html="1"$>images/favicon.ico">
<link rel="start" href="<$mt:BlogURL encode_html="1"$>">
<link rel="alternate" type="application/atom+xml" title="Recent Entries" href="<$mt:Link template="feed_recent"$>">
<$mt:CanonicalLink$>

<!-- Open Graph Protocol -->
<meta property="og:type" content="article">
<meta property="og:locale" content="<$mt:BlogLanguage setvar="blog_lang"$><mt:If name="blog_lang" eq="ja">ja_JP<mt:Else><$mt:Var name="blog_lang"$></mt:If>">
<meta property="og:title" content="<$mt:PageTitle encode_html="1"$> <mt:If tag="pagefolder">| <mt:PageFolder><$mt:FolderLabel encode_html="1"$></mt:PageFolder> </mt:If>| <$mt:BlogName encode_html="1"$>">
<meta property="og:url" content="<$mt:PagePermalink encode_html="1"$>">
<meta property="og:description" content="<$mt:BlogDescription remove_html="1" encede_html="1"$>">
<meta property="og:site_name" content="<$mt:BlogName encode_html="1"$>">
<meta property="og:image" content="<$mt:BlogURL encode_html="1"$>images/ogimage.png">
<!-- Microdata -->
<meta itemprop="description" content="<$mt:BlogDescription remove_html="1" encede_html="1"$>">
<link itemprop="url" href="<$mt:PagePermalink encode_html="1"$>">
<link itemprop="image" href="<$mt:BlogURL encode_html="1"$>images/ogimage.png">

<link href='http://fonts.googleapis.com/css?family=Marmelad' rel='stylesheet' type='text/css'>
<link rel="stylesheet" href="http://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css">
<link rel="stylesheet" href="<$mt:BlogURL encode_html="1"$>css/normalize.css">
<link rel="stylesheet" href="<$mt:BlogURL encode_html="1"$>css/foundation.min.css">
<link rel="stylesheet" href="<$mt:BlogURL encode_html="1"$>styles.css">
<script src="<$mt:BlogURL encode_html="1"$>js/modernizr.js"></script>
</head>

<body class="webpage">
<mt:SetVarBlock name="thisfolder"><mt:PageFolder><$mt:FolderLabel$></mt:PageFolder></mt:SetVarBlock>
<mt:SetVarBlock name="thispage"><$mt:PageTitle$></mt:SetVarBlock>
<div class="off-canvas-wrap">
<div class="inner-wrap">

<$mt:Include module="header-menu"$>

<div class="row">
  <div class="columns medium-12">
    <nav role="navigation">
      <ul class="breadcrumbs">
        <li class="home"><a href="<$mt:BlogRelativeURL$>"><i class="fa fa-home fa-lg"></i></a></li>
        <mt:If name="thisfolder">
        <li class="has-dropdown">
          <a data-dropdown="<$mt:FolderBasename$>-bc" aria-controls="<$mt:FolderBasename$>-bc" aria-expanded="false"><mt:PageFolder><$mt:FolderLabel$></mt:PageFolder> <i class="fa fa-caret-down fa-sm"></i></a>
          <ul id="<$mt:FolderBasename$>-bc" class="f-dropdown" data-dropdown-content aria-hidden="true" tabindex="-1">
            <mt:Pages folder="$thisfolder" sort_order="ascend">
            <mt:SetVarBlock name="pagetitle"><$mt:PageTitle$></mt:SetVarBlock>
            <li<mt:If name="thispage" eq="$pagetitle"> class="active"</mt:If>><a href="<$mt:PagePermalink$>"><$mt:PageTitle$></a></li>
            </mt:Pages>
          </ul>
        </li>
        </mt:If>
        <li><span><$mt:PageTitle$></span></li>
      </ul>
    </nav>
   </div>
 </div>

<div class="row">
  <div class="columns medium-12">
    <div id="mainvisual">
      <img src="<$mt:BlogURL encode_html="1"$>images/slide01.jpg" alt="" width="0" height="0">
      <div class="page-banner">
        <div class="inner">
        <h2><i class="fa fa-sun-o"></i> <$mt:PageTitle$></h2>
        </div>
      </div>
    </div><!-- mainvisual -->
  </div><!-- columns -->
</div><!-- row -->

<div class="row">
  <div class="columns medium-8 large-8">
    <div id="content">
      <article id="webpage-body">
        <div class="container">
        <$mt:PageBody$>
        <$mt:PageMore$>
        </div>
      </article>
    </div>
  </div><!-- columns -->
  <div class="columns medium-4 large-4">
    <$mt:Include module="information"$>
  </div><!-- columns -->
</div><!-- row -->

<$mt:Include module="footer"$>

<$mt:Include module="left-menu"$>

</div><!-- inner-wrap -->
</div><!-- off-canvas-wrap -->

<$mt:Include module="footer-script"$>
</body>
</html>
